{% extends 'base.html.twig' %}

{% block main %}
    {% for question in questions %}
        <p>{{ question.title }}</p>
        <p>{{ question.content }}</p>
        <p>{{ question.author.fullName }}</p>
        <p>{{ question.dateAdded|date("F jS \\a\\t g:ia") }}</p>
    {% endfor %}
{% endblock %}

